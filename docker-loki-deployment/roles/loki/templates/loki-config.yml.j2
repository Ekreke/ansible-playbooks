auth_enabled: false
server:
  http_listen_port: 3100
ingester:
  lifecycler:
    ring:
      replication_factor: 1
storage_config:
  filesystem:
    directory: /tmp/loki
schema_config:
  configs:
    - from: 2024-01-01
      store: boltdb-shipper
      object_store: filesystem
      schema: v11
      index:
        prefix: index_
        period: 168h
